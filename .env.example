# Environment variables for local development
# Copy this to .env.local and fill in your values

# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api
VITE_USE_MOCK_API=true

# AWS Configuration (will be filled after AWS setup)
VITE_AWS_REGION=ap-south-2
VITE_AWS_COGNITO_USER_POOL_ID=
VITE_AWS_COGNITO_CLIENT_ID=
VITE_AWS_API_GATEWAY_URL=

# Feature Flags
VITE_ENABLE_WEBHOOK_TRIGGERS=true
VITE_ENABLE_SCHEDULED_TRIGGERS=true
VITE_ENABLE_EMAIL_ACTIONS=true

# App Configuration
VITE_APP_NAME=SpyGlass
VITE_APP_VERSION=1.0.0
